PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pmdcat: <http://publishmydata.com/pmdcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>

CONSTRUCT {
  ?observation ?p ?o .
  ?observation pmdcat:graph ?graph .
} WHERE {
  GRAPH ?graph {
    ?observation ?p ?o .
  }
}
